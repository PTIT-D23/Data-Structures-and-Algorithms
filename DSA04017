#include<bits/stdc++.h>
//#include<iostream>
using namespace std ;
#define int long long
const int maxn = 1e7 + 7;
const int mod = 1e9 + 7;

int32_t main()
	{
        ios_base::sync_with_stdio(0);
        cin.tie(0); cout.tie(0);
		int t; cin >> t;
		while(t--)
			{
				int n; cin >> n;
				int a[n], b[n];
				for(int &x : a) cin >> x;
				for(int i = 0; i < n - 1; i++)
					cin >> b[i];
				for(int i = 0; i < n; i++)
					{
						if(a[i] != b[i])
							{
								cout << i + 1 << endl;
								break;
							}
							
					}
			}
		return 0;
	}
