#include<bits/stdc++.h>
//#include<iostream>
using namespace std ;
#define int long long
const int maxn = 1e7 + 7;
const int mod = 1e9 + 7;

int32_t main()
	{
        ios_base::sync_with_stdio(0);
        cin.tie(0); cout.tie(0);
		int t; cin >> t;
		while(t--)
			{
				int n, m; cin >> n >> m;
				multiset<int> se;
				for(int i = 0; i < n + m ;i++)
					{
						int x; cin >> x;
						se.insert(x);
					}
				for(int x: se) cout << x <<' ';
				cout<< endl;	
			}
		return 0;
	}
