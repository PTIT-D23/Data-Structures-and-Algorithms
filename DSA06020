#include<bits/stdc++.h>
//#include<iostream>
using namespace std ;
#define int long long
const int maxn = 1e6 + 7;
const int mod = 1e9 + 7;

int32_t main()
	{
        ios_base::sync_with_stdio(0);
        cin.tie(0); cout.tie(0);
		int t; cin >> t;
		while(t--)
			{
				int n, k; cin >> n >> k;
				unordered_multiset<int> se;
				for(int i = 0; i < n ;i++)
					{
						int x; cin >> x;
						se.insert(x);
					}
				if(se.find(k) != se.end())
					cout <<"1\n";
				else cout <<"-1\n";
			}
		return 0;
	}
