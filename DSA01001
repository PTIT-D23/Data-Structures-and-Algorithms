#include<bits/stdc++.h>
//#include<iostream>
using namespace std ;
#define int long long
const int maxn = 1e7 + 7;
const int mod = 1e9 + 7;

int32_t main(){
    ios_base::sync_with_stdio(0);
    cin.tie(0); cout.tie(0);
	int t; cin >> t;
	while(t--){
		string s; cin >> s;
		int i = s.size() - 1;
		while(i >= 0 && s[i] == '1'){
			s[i] = '0';
			i--;
		}
		if(i != -1)
			s[i] = '1';
		cout << s << endl;
	}
	return 0;
}
