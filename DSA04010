#include<bits/stdc++.h>
//#include<iostream>
using namespace std ;
#define int long long
const int maxn = 1e7 + 7;
const int mod = 1e9 + 7;

void dp(int a[], int n)
	{
		int s = 0, e = 0;
		for(int i = 0; i < n; i++)
			{
				e = max(a[i], e + a[i]);
				s = max(s, e);
			}
		cout << s << endl;
	}
int32_t main()
	{
        ios_base::sync_with_stdio(0);
        cin.tie(0); cout.tie(0);
		int t; cin >> t;
		while(t--)
			{
				int n; cin >> n;
				int a[n]; for(int &x: a) cin >> x;
				dp(a, n);
			}
		return 0;
	}
