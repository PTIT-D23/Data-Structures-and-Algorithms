#include<bits/stdc++.h>
//#include<iostream>
using namespace std ;
#define int long long
const int maxn = 1e7 + 7;
const int mod = 1e9 + 7;

int32_t main()
	{
        ios_base::sync_with_stdio(0);
        cin.tie(0); cout.tie(0);
		int t; cin >> t;
		while(t--)
			{
				int n, k; cin >> n >> k;
				map<int, int > mp;
				for(int i = 1; i <= n ;i++)
					{
						int x; cin >> x;
						mp[x]++;
					}
				if(mp[k] != 0)
					cout << mp[k] << endl;
				else cout <<"-1\n";
			}
		return 0;
	}
