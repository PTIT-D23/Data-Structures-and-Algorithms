#include<bits/stdc++.h>
//#include<iostream>
using namespace std ;
#define int long long
const int maxn = 1e7 + 7;
const int mod = 1e9 + 7;

int32_t main()
	{
        ios_base::sync_with_stdio(0);
        cin.tie(0); cout.tie(0);
		int t; cin >> t;
		while(t--)
			{
				int n, k; cin >> n >> k;
				int a[n];
				for(int &x : a) cin >> x;
				sort(a,a+n);
				int j = n - 1;
				for(int i = 0; i < k; i++)
					{
						cout << a[j--] << ' ';
					}
				cout << endl;
			}
		return 0;
	}
